    "lintfix": "eslint src --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.2.3",
